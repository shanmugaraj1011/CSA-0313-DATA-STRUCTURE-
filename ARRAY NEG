#include<stdio.h>
#include<conio.h>
int main()
{
int a[10],i,n;
printf("enter the number of elements");
scanf("%d",&n);
printf("ener the elements one by one;");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("\nnegative numbers");
for(i=0;i<n;i++)
{
if(a[i]<0)
{
printf("%d",a[i]);
//getch()
return 0;
}
}
}
